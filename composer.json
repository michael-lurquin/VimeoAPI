{
    "name": "michael-lurquin/vimeo-api",
    "description": "Laravel Vimeo API",
    "keywords": ["laravel", "vimeo", "api"],
    "homepage": "https://github.com/michael-lurquin/VimeoAPI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MichaÃ«l Lurquin",
            "email": "michael.lurquin@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "MichaelLurquin\\VimeoApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichaelLurquin\\VimeoApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "MichaelLurquin\\VimeoApi\\ServiceProvider"
            ],
            "aliases": {
                "Vimeo": "MichaelLurquin\\VimeoApi\\Facade"
            }
        }
    }
}
