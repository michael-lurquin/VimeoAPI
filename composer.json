{
    "name": "michael-lurquin/vimeo-api",
    "description": "Laravel Vimeo API",
    "keywords": ["laravel", "vimeo", "api"],
    "homepage": "https://github.com/michael-lurquin/VimeoAPI",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MichaÃ«l Lurquin",
            "email": "michael.lurquin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^7.1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.28.1",
        "phpunit/phpunit": "^8.5.21"
    },
    "autoload": {
        "psr-4": {
            "MichaelLurquin\\Vimeo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichaelLurquin\\Vimeo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichaelLurquin\\Vimeo\\VimeoServiceProvider"
            ]
        }
    }
}
